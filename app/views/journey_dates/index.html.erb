<h1>Affichage dates propos√©es</h1>

<% @journey_dates.each do |journey_date| %>
    <li><%= journey_date.start_date%></li>
    <li><%= journey_date.end_date%></li>
<% end %>
<%= link_to "Ce choix me convient", journey_journey_accommodations_path(@journey) %>

<%= form_with model: [@journey, @journey_date] do |form| %>
  <%= form.label :start_date %>
  <%= form.date_field :start_date %>
  <%= form.label :end_date %>
  <%= form.date_field :end_date %>
  <%= form.submit "Proposer une date" %>
<% end %>

